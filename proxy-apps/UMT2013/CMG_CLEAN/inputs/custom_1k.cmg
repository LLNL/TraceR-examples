# processor block decomposition
sms(16,8,8)

# Always specify blocks in block base numbering
blk(on,0:15,0:7,0:7)
numzones(8,8,10)

# tag boundary faces
tag("xMinFaces",face,(0:0,0:8,0:8))
tag("xMaxFaces",face,(16:16,0:8,0:8))
tag("yMinFaces",face,(0:16,0:0,0:8))
tag("yMaxFaces",face,(0:16,8:8,0:8))
tag("zMinFaces",face,(0:16,0:8,0:0))
tag("zMaxFaces",face,(0:16,0:8,8:8))

# Hex subdivisions
sub(10%,0:15,0:7,0:7,(7,0,0,0)) #7 hex
seed(10)
